  <article>
    
    <div class="clearfix">{{ content }}</div>
    
    <img src="../assets/img/webpage_logo.png" alt="drawing" width="400"/>

    <div class="Lab_summary" style="text-align: center">
      <h1> Future of Medical Robots</h1>
      
      <p> We develop next generation of medical robotics systems focusing on novel actuation and sensing methods.</p>
      <br>  
      <a href="/Projects/">Learn More</a>
    </div>

    <div class="Hiring" style="text-align: center">

      <h1> Looking for Teammates!</h1>
      <p>We are continuously looking for talented Master's, Doctoral students, and Postdoctoral researchers, as well as motivated undergraduate researchers/interns. </p>
      <br>
      <a href="/Opportunities/index.html">Opportunities</a>


    </div>

    <!-- News -->
    {% if page.announcements and page.announcements.enabled %}
      <h2>
        <a href="{{ '/news/' | relative_url }}" style="color: inherit">news</a>
      </h2>
      {% include news.liquid limit=true %}
    {% endif %}


    <!-- Selected papers -->
    {% if page.selected_papers %}
      <h2>
        <a href="{{ '/publications/' | relative_url }}" style="color: inherit">selected publications</a>
      </h2>
      {% include selected_papers.liquid %}
    {% endif %}

    <!-- Social -->
    {% if page.social %}
      <div class="social">
        <div class="contact-icons">{% include social.liquid %}</div>

        <div class="contact-note">{{ site.contact_note }}</div>
      </div>
    {% endif %}
  </article>
</div>
